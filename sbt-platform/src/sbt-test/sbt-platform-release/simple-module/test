# Doing this because sbt-platform needs a well-formed git repository
$ exec git init .
$ exec git remote add origin https://github.com/scalaplatform/dummy
$ exec git fetch
$ exec git checkout -t origin/empty
$ exec git add .
$ exec git commit -m "test"

> show version
> show useGpg
> inspect pgpPassphrase
> inspect pgpPublicRing
> inspect pgpSecretRing
> inspect releaseEarlyNoGpg

> inspect platformRootDir
> inspect platformInsideCi

> show pgpPassphrase
> show pgpPublicRing
> show pgpSecretRing
> compile

> run
$ exists target/scala-2.11

> releaseEarly
